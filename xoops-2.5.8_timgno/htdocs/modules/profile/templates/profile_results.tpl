<{includeq file="db:profile_breadcrumbs.html"}><div>( <{$total_users}> )</div><{if $users}>    <table class="table table-bordered">        <thead>				<tr>            <{foreach item=caption from=$captions}>                <th><{$caption}></th>            <{/foreach}>			</tr>		</thead>        <tbody>		<{foreach item=user from=$users}>            <tr class="<{cycle values='odd, even'}>">                <{foreach item=fieldvalue from=$user.output}>                    <td><{$fieldvalue}></td>                <{/foreach}>            </tr>        <{/foreach}>		</tbody>    </table>        <{$nav}><{else}>    <div class="alert alert-danger">        <{$smarty.const._PROFILE_MA_NOUSERSFOUND}>    </div><{/if}>